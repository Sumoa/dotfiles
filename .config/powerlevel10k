POWERLEVEL9K_SHORTEN_DIR_LENGTH=2
POWERLEVEL9K_VCS_GIT_HOOKS=(vcs-detect-changes git-untracked git-aheadbehind git-remotebranch git-tagname)

POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(battery time command_execution_time dir_writable)

POWERLEVEL9K_BATTERY_HIDE_ABOVE_THRESHOLD=95
POWERLEVEL9K_TIME_FORMAT="%D{%d.%m %H:%M:%S}"


POWERLEVEL9K_LEFT_SEGMENT_SEPARATOR="\uE0B4"
POWERLEVEL9K_LEFT_SUBSEGMENT_SEPARATOR="\uE0B5"
